doctype html(lang='pt-br')
html(ng-app="app")
    head
        meta(charset='utf-8')
        meta(http-equiv='X-UA-Compatible', content='IE=edge')
        meta(name='viewport', content='width=device-width, initial-scale=1')
        // The above 3 meta tags *must* come first in the head; any other head content must come *after* these tags
        title Matheus Paludo | Consultoria Juridica
        link(rel='stylesheet', href='/stylesheets/estilo.css')
        link(rel='stylesheet', href='/stylesheets/estilo_painel.css')
        // Bootstrap 3.3.5
        link(rel='stylesheet', href='/bootstrap/css/bootstrap.min.css')
        // Font Awesome
        link(rel='stylesheet', href='https://maxcdn.bootstrapcdn.com/font-awesome/4.4.0/css/font-awesome.min.css')
        // Ionicons
        link(rel='stylesheet', href='https://code.ionicframework.com/ionicons/2.0.1/css/ionicons.min.css')
        link(rel='stylesheet', href='/dist/css/skins/_all-skins.min.css')
        // Theme style
        link(rel='stylesheet', href='/dist/css/AdminLTE.min.css')

    body(data-spy='scroll', data-target='.menu-navegacao', data-offset='80')
        nav.navbar.navbar-default.navbar-fixed-top
            .container-fluid
                .navbar-header
                    button.navbar-toggle.collapsed(type='button', data-toggle='collapse', data-target='#bs-example-navbar-collapse-1', aria-expanded='false')
                        span.sr-only Toggle navigation
                        span.icon-bar
                        span.icon-bar
                        span.icon-bar
                    a.navbar-brand(href='#page-top') Logo
                #bs-example-navbar-collapse-1.collapse.navbar-collapse.navbar-right.menu-navegacao
                    ul.nav.navbar-nav
                        li
                            a(href='#page-top')
                        li.active
                            a(href='/') Home
                        li
                            a(href='#servicos') Servicos
                        li
                            a(href='#portifolio') Portifolio
                        li
                            a(href='#quemsomos') Quem Somos
                        li
                            a(href='#localizacao') Localizacao
                        li
                            a(href='#contato') Contato
        block content
        footer.navbar-inverse
            .container
                .row
                    .col-xs-12.col-sm-4.col-md-4.text-left.end-rodape
                        address
                            strong Matheus Paludo Consultoria Juridica
                            br
                            strong R. S&atilde;o Leopoldo, 429 - Vila Jardim
                            br
                            strong Porto Alegre RS, CEP 91330-690
                            br
                            abbr(title='Telefone') Tel: (51) 92678620
                    .col-xs-12.col-sm-4.col-md-4.text-center
                        .icon-rodape
                            .col-xs-3
                                a(href='', title='Facebook', alt='Facebook')
                                    img.img-responsive(src='/images/icones/facebook.png')
                                br
                                a(href='', title='Twitter', alt='Twitter')
                                    img.img-responsive(src='/images/icones/twitter.png')
                            .col-xs-3
                                a(href='', title='Youtube', alt='Youtube')
                                    img.img-responsive(src='/images/icones/youtube.png')
                                br
                                a(href='', title='Linkedin', alt='Linkedin')
                                    img.img-responsive(src='/images/icones/linkedin.png')
                            .col-xs-3
                                a(href='', title='Skipe', alt='Skipe')
                                    img.img-responsive(src='/images/icones/skype.png')
                                br
                                a(href='', title='Googleplus', alt='Googleplus')
                                    img.img-responsive(src='/images/icones/googleplus.png')
                    .col-xs-12.col-sm-4.col-md-4.text-right.rodape-direita
                        address
                            strong Camus | Desenvolvimento Web
                            br
                            strong Site: camusdesenvolvimentoweb.com.br
                            br
                            abbr(title='Telefone') Tel: (51) 9975-9596

        script(src='/plugins/jQuery/jQuery-2.1.4.min.js')

        // INICIO ANGULARJS
        script(src='/javascripts/angular/angular.min.js')
        script(src="/socket.io/socket.io.js")
        script(src='/javascripts/angular/ng-socket-io.js')
        script(src='/javascripts/app.module.js')
        script(src='/javascripts/clienteChatController.js')
        // FIM ANGULARJS

        script(type='text/javascript', src='/javascripts/jquery.validate.js')
        script(type='text/javascript', src='/javascripts/jquery.mask.js')
        script(type='text/javascript', src='/javascripts/bootstrap.min.js')
        script(type='text/javascript', src='/javascripts/main.js')
